{
    "multipart": [
        {   "when": {"is_master": false},
            "apply": { "model": "cm:block/cable_block_post" }
        },
        {   "when": { "north": "true" },
            "apply": { "model": "cm:block/cable_block_side", "uvlock": true }
        },
        {   "when": { "east": "true" },
            "apply": { "model": "cm:block/cable_block_side", "y": 90, "uvlock": true }
        },
        {   "when": { "south": "true" },
            "apply": { "model": "cm:block/cable_block_side", "y": 180, "uvlock": true }
        },
        {   "when": { "west": "true" },
            "apply": { "model": "cm:block/cable_block_side", "y": 270, "uvlock": true }
        }
    ]
}
